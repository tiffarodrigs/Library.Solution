@{
  Layout = "_Layout";
}
<div class="container">
  @await Html.PartialAsync("_Navbar")
  <div class="card mb-3">
    <div class="card-body">
      <h5 class="card-title">Welcome to LibraryWare: Modern Library Management.</h5>
      <p class="card-text">Click on the links above to get started.</p>
    </div>
    <img id="homepage-photo" class="card-img-bottom" src="~/img/homepage_artwork.jpg" alt="photo of library">
    <p>@Html.ActionLink("Create an account", "Index", "Account")</p>
  </div>
  @await Html.PartialAsync("_Footer")
</div>

